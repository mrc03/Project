        <item name="colorPrimary">#303F9F</item>
        <item name="colorPrimaryDark">#303F9F</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>
    <style name="MatchSpinnerStyle" parent="android:style/Widget.ListView.DropDown">
